@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <!-- Core CSS - Include with every page -->
   
    <link href="~/Content/themes/admin/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/themes/admin/font-awesome/css/font-awesome.css" rel="stylesheet">

    <!-- Page-Level Plugin CSS - Dashboard -->
    <link href="~/Content/themes/admin/css/plugins/morris/morris-0.4.3.min.css" rel="stylesheet">
    <link href="~/Content/themes/admin/css/plugins/timeline/timeline.css" rel="stylesheet">

    <!-- SB Admin CSS - Include with every page -->
    <link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/themes/admin/css/sb-admin.css" rel="stylesheet">
    <link href="~/Content/themes/admin/css/custom.css" rel="stylesheet">
    <!-- Page-Level Plugin CSS - Tables -->
    <link href="~/Content/themes/admin/css/plugins/dataTables/dataTables.bootstrap.css" rel="stylesheet">

</head>
<body>
    <div id="wrapper">
        @{Html.RenderPartial("_TopNavBar");}
        <!-- /.navbar-static-top -->
        <!-- /.navbar-static-top -->
        @{Html.RenderAction("GetMainMenu", "Navigation");}
        <!-- /.navbar-static-side -->

        <div id="page-wrapper">
            @RenderBody()
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.8.24.min.js"></script>
    <script src="~/Scripts/admin/bootstrap.min.js"></script>
    <script src="~/Scripts/plugins/metisMenu/jquery.metisMenu.js"></script>
    <!-- SB Admin Scripts - Include with every page -->
    <script src="~/Scripts/admin/sb-admin.js"></script>
    <!-- Page-Level Plugin Scripts - Tables -->
    <script src="~/Scripts/plugins/dataTables/jquery.dataTables.js"></script>
    <script src="~/Scripts/plugins/dataTables/dataTables.bootstrap.js"></script>
    <script src="~/Scripts/dialogmessage.js"></script>
    <script src="~/Scripts/Plugins/ckfinder/ckfinder.js" type="text/javascript"></script>
    <script src="~/Scripts/Plugins/ckeditor/ckeditor.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="~/Scripts/admin/datatable_display.js"></script>
    <script src="~/Scripts/admin/ck_editor_scripts.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
