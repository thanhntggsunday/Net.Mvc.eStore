@model NetMvc.Cms.Common.ViewModel.Common.ApiResult<NetMvc.Cms.Common.Dto.FeedbackDto>

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">@Resources.CmsResource.AdminFeedbackDetailTitlePage</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                @Resources.CmsResource.AdminNoteForm
            </div>
            <div class="panel-body">

                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.ResultObj.Name)</label>
                            @Html.DisplayFor(model => model.ResultObj.Name)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.ResultObj.Company)</label>
                            @Html.DisplayFor(model => model.ResultObj.Company)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.ResultObj.Address)</label>
                            @Html.DisplayFor(model => model.ResultObj.Address)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.ResultObj.Phone)</label>

                            @Html.DisplayFor(model => model.ResultObj.Phone)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.ResultObj.Email)</label>

                            @Html.DisplayFor(model => model.ResultObj.Email)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.ResultObj.Title)</label>

                            @Html.DisplayFor(model => model.ResultObj.Title)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.ResultObj.Message)</label>

                            @Html.DisplayFor(model => model.ResultObj.Message)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.ResultObj.CreatedDate)</label>

                            @Html.DisplayFor(model => model.ResultObj.CreatedDate)
                        </div>

                        <div class="form-group">
                            <label>@Html.LabelFor(model => model.ResultObj.IsReaded)</label>
                            @Html.DisplayFor(model => model.ResultObj.IsReaded)
                        </div>
                        @Html.ActionLink(@Resources.CmsResource.GoBackToListText, "Index");
                    </div>
                </div>
                <!-- /.row (nested) -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
